#include <iostream>

using namespace std;

bool imp(bool a, bool b) {
	if ((a == 1) && (b == 0)) {
		return 0;
	}
	else {
		return 1;
	}
}
int main() {
	cout << "Enter x, y and z" << endl;
	bool x, y, z, p1, p2, p3, p4, p5, ans;
	cin >> x >> y >> z;
	p1 = (x || !y );
	p2 = (y && !z );
	p3 = (imp(y, z) && imp(z, y));
	p4 = (x || (p3));
	p5 = imp(p2, p4);
	ans = imp(p1, p5);
	cout << "Answer is: " << ans << endl;
	return 0;
}
